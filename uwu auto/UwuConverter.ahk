    static UwuTransform() {
        if !this.isActive || this.IsBlacklisted()
            return
        
        ; –°–æ—Ö—Ä–∞–Ω—è–µ–º clipboard
        oldClip := A_Clipboard
        A_Clipboard := ""
        
        ; –ö–æ–ø–∏—Ä—É–µ–º –≤—ã–¥–µ–ª–µ–Ω–Ω—ã–π —Ç–µ–∫—Å—Ç
        Send("^c")
        if !ClipWait(0.5) {
            ; –ï—Å–ª–∏ –Ω–∏—á–µ–≥–æ –Ω–µ –≤—ã–¥–µ–ª–µ–Ω–æ - —Ç—Ä–∞–Ω—Å—Ñ–æ—Ä–º–∏—Ä—É–µ–º –ø–æ—Å–ª–µ–¥–Ω–µ–µ —Å–ª–æ–≤–æ
            this.TransformLastWord()
            return
        }
        
        text := A_Clipboard
        if text = "" {
            A_Clipboard := oldClip
            return
        }
        
        ; –¢—Ä–∞–Ω—Å—Ñ–æ—Ä–º–∞—Ü–∏—è
        transformed := this.ProcessText(text)
        
        ; –í—Å—Ç–∞–≤–ª—è–µ–º –æ–±—Ä–∞—Ç–Ω–æ
        A_Clipboard := transformed
        Send("^v")
        
        Sleep(100)
