        Sleep(50)
        
        oldClip := A_Clipboard
        A_Clipboard := ""
        Send("^c")
        
        if !ClipWait(0.3)
            return
        
        word := A_Clipboard
        transformed := this.ProcessText(word)
        
        A_Clipboard := transformed
        Send("^v")
        
        Sleep(100)
        A_Clipboard := oldClip
    }
    
