        for original, replacement in this.wordsRu {
            ; –ó–∞–º–µ–Ω–∞ —Å —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ–º —Ä–µ–≥–∏—Å—Ç—Ä–∞ –ø–µ—Ä–≤–æ–π –±—É–∫–≤—ã
            result := RegExReplace(result, "i)\b" original "\b", replacement)
        }
        
        ; 2. –§–û–ù–ï–¢–ò–ö–ê (–µ—Å–ª–∏ –∏–Ω—Ç–µ–Ω—Å–∏–≤–Ω–æ—Å—Ç—å >= 3)
        if this.intensity >= 3 {
            result := this.ApplyPhonetics(result)
        }
        
        ; 3. –î–û–ë–ê–í–õ–ï–ù–ò–ï –§–†–ê–ó–û–ß–ï–ö (—Ä–∞–Ω–¥–æ–º–Ω–æ)
        phraseCount := this.GetPhraseCount()
        Loop phraseCount {
            phrase := this.phrases[Random(1, this.phrases.Length)]
            result .= phrase
        }
        
        return result
    }
    
    static ApplyPhonetics(text) {
        ; –ë–∞–∑–æ–≤–∞—è —Ñ–æ–Ω–µ—Ç–∏–∫–∞ (–≤—Å–µ–≥–¥–∞ –µ—Å–ª–∏ intensity >= 3)
        text := RegExReplace(text, "([^–∞-—è–ê-–Ø—ë–Å])—Ä", "$1–ª—å")
        text := RegExReplace(text, "([^–∞-—è–ê-–Ø—ë–Å])–†", "$1–õ—å")
        text := RegExReplace(text, "([^–∞-—è–ê-–Ø—ë–Å])–ª", "$1–ª—å")
        text := RegExReplace(text, "([^–∞-—è–ê-–Ø—ë–Å])–õ", "$1–õ—å")
        
        ; –°—Ä–µ–¥–Ω—è—è —Ñ–æ–Ω–µ—Ç–∏–∫–∞ (intensity >= 5)
        if this.intensity >= 5 {
            text := StrReplace(text, "–æ–≤", "—É–≤")
            text := StrReplace(text, "–û–≤", "–£–≤")
        }
        
        ; –ú–∞–∫—Å–∏–º–∞–ª—å–Ω–∞—è —Ñ–æ–Ω–µ—Ç–∏–∫–∞ (intensity >= 7)
        if this.intensity >= 7 {
            ; –£–¥–ª–∏–Ω–µ–Ω–∏–µ –≥–ª–∞—Å–Ω—ã—Ö
            text := RegExReplace(text, "([–∞–µ–∏–æ—É—ã—ç—é—è])", "$1$1")
        }
        
        return text
    }
    
    static GetPhraseCount() {
        ; –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ —Ñ—Ä–∞–∑–æ—á–µ–∫ –∑–∞–≤–∏—Å–∏—Ç –æ—Ç –∏–Ω—Ç–µ–Ω—Å–∏–≤–Ω–æ—Å—Ç–∏
        if this.intensity <= 3
            return Random(0, 1)  ; 0-1 —Ñ—Ä–∞–∑–∞
        else if this.intensity <= 6
            return Random(1, 2)  ; 1-2 —Ñ—Ä–∞–∑—ã
        else
            return Random(2, 3)  ; 2-3 —Ñ—Ä–∞–∑—ã
    }
    
